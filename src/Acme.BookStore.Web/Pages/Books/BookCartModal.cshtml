@page
@using Acme.BookStore.Localization;
@using Acme.BookStore.Web.Pages.Books;
@using Microsoft.Extensions.Localization;
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal;  
@model Acme.BookStore.Web.Pages.Books.BookCartModalModel
@inject IStringLocalizer<BookStoreResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="lstBookCart" asp-page="/Books/BookCartModal">
    <abp-modal>
        <abp-modal-header title="@L["BookCart"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
